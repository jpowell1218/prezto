function integrate-this() {
    BRANCH=$(git rev-parse --abbrev-ref HEAD)
    PROJECT=${PWD##*/}
    CURRENT_DIR=$PWD
    cd ~/Code/captainu/_integration/$PROJECT
    git pull origin master
    git pull $CURRENT_DIR $BRANCH
    git push origin master
    cd $CURRENT_DIR
    BRANCH=''
    CURRENT_DIR=''
    cap staging deploy
}
